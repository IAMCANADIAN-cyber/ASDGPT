+            "state_estimation": {
+                "arousal": 70
+            }
+        }
+        engine.update(new_state)
+        state = engine.get_state()
+        self.assertEqual(state['arousal'], 70)
+        self.assertEqual(state['focus'], 50) # Should remain unchanged
