+        # 2. Immediate update (t=101) - should NOT log (interval 10s)
+        with patch('time.time', return_value=101.0):
+            self.engine.update()
+            self.mock_logger.log_debug.assert_not_called()
+
+        # 3. Later update (t=111) - should log
