+import pytest
+import time
+import json
+import os
+import shutil
+from unittest.mock import MagicMock
+from core.intervention_engine import InterventionEngine
+import config
+
+# Create a temporary test directory for user data
+TEST_USER_DATA_DIR = "test_feedback_loop_data_v2"
