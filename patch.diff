+TEST_PREFERENCES_FILE = os.path.join(TEST_USER_DATA_DIR, "preferences.json")
+
+@pytest.fixture
+def setup_test_env():
+    # Setup
+    os.makedirs(TEST_USER_DATA_DIR, exist_ok=True)
+    # Override config paths for testing
+
+    # Store original config values
