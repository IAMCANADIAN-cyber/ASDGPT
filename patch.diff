+        self.assertEqual(state['mood'], 50)
+        self.assertEqual(engine.history_size, 5)
+
+    def test_update_valid_state(self):
+        engine = StateEngine(history_size=1)
+        new_state = {
+            "state_estimation": {
+                "arousal": 60,
+                "overload": 10,
+                "focus": 80,
+                "energy": 70,
+                "mood": 60
+            }
