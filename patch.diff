+        self.assertEqual(metrics["posture_state"], "slouching")
+
+    def test_no_face_detected(self):
+        frame = np.zeros((100, 100, 3), dtype=np.uint8)
+        self.sensor.face_cascade.detectMultiScale.return_value = []
+
