+
+        # Test default (empty dict)
+        tray.update_tooltip({})
+        expected_initializing = f"{config.APP_NAME}\nInitializing..."
+        self.assertEqual(tray.tray_icon.title, expected_initializing)
+
+        # Test with full state (5D)
+        state = {
+            "arousal": 65,
+            "overload": 20,
+            "focus": 80,
+            "energy": 42,
+            "mood": 55
+        }
+        tray.update_tooltip(state)
